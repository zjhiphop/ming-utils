#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mrx/Documents/work/ming/repo/labs/ming-cli/node_modules/.pnpm/mocha@9.1.4/node_modules/mocha/bin/node_modules:/Users/mrx/Documents/work/ming/repo/labs/ming-cli/node_modules/.pnpm/mocha@9.1.4/node_modules/mocha/node_modules:/Users/mrx/Documents/work/ming/repo/labs/ming-cli/node_modules/.pnpm/mocha@9.1.4/node_modules:/Users/mrx/Documents/work/ming/repo/labs/ming-cli/node_modules/.pnpm/node_modules:/Users/mrx/Documents/work/ming/repo/labs/ming-cli/node_modules:/Users/mrx/Documents/work/ming/repo/labs/node_modules:/Users/mrx/Documents/work/ming/repo/node_modules:/Users/mrx/Documents/work/ming/node_modules:/Users/mrx/Documents/work/node_modules:/Users/mrx/Documents/node_modules:/Users/mrx/node_modules:/Users/node_modules:/node_modules:/Users/mrx/Documents/work/ming/repo/labs/ming-cli/packages/@ming/utils/node_modules:/Users/mrx/Documents/work/ming/repo/labs/ming-cli/packages/@ming/node_modules:/Users/mrx/Documents/work/ming/repo/labs/ming-cli/packages/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/mrx/Documents/work/ming/repo/labs/ming-cli/node_modules/.pnpm/mocha@9.1.4/node_modules/mocha/bin/node_modules:/Users/mrx/Documents/work/ming/repo/labs/ming-cli/node_modules/.pnpm/mocha@9.1.4/node_modules/mocha/node_modules:/Users/mrx/Documents/work/ming/repo/labs/ming-cli/node_modules/.pnpm/mocha@9.1.4/node_modules:/Users/mrx/Documents/work/ming/repo/labs/ming-cli/node_modules/.pnpm/node_modules:/Users/mrx/Documents/work/ming/repo/labs/ming-cli/node_modules:/Users/mrx/Documents/work/ming/repo/labs/node_modules:/Users/mrx/Documents/work/ming/repo/node_modules:/Users/mrx/Documents/work/ming/node_modules:/Users/mrx/Documents/work/node_modules:/Users/mrx/Documents/node_modules:/Users/mrx/node_modules:/Users/node_modules:/node_modules:/Users/mrx/Documents/work/ming/repo/labs/ming-cli/packages/@ming/utils/node_modules:/Users/mrx/Documents/work/ming/repo/labs/ming-cli/packages/@ming/node_modules:/Users/mrx/Documents/work/ming/repo/labs/ming-cli/packages/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../node_modules/.pnpm/mocha@9.1.4/node_modules/mocha/bin/_mocha" "$@"
else
  exec node  "$basedir/../../../../../node_modules/.pnpm/mocha@9.1.4/node_modules/mocha/bin/_mocha" "$@"
fi
